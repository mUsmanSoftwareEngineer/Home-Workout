<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="12dp" />
            <gradient
                android:angle="270"
                android:centerColor="@color/gray_light_"
                android:centerY="0.75"
                android:endColor="@color/gray_light_"
                android:startColor="@color/gray_light_" />
        </shape>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="12dp" />
                <gradient
                    android:angle="0"
                    android:endColor="@color/green_seven_fit"
                    android:startColor="@color/green_seven_fit" />
            </shape>
        </clip>
    </item>

</layer-list>